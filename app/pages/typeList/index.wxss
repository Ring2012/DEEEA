/* pages/typeList/index.wxss */

.page {
  background: #e9e9e9;
}

.type-image {
  width: 750rpx;
  height: 320rpx;
  margin-bottom: 5rpx;
}

.typeList {
  width: 100%;
  background: #fff;
  padding-top: 20rpx;
}

.tp-list {
  width: 50%;
  display: inline-block;
  margin-bottom: 20rpx;
}

.tp-img {
  display: block;
  width: 320rpx;
  height: 320rpx;
  margin: 0 auto;
}

.tp-text {
  width: 320rpx;
  margin: 0 auto;
}

.tp-title, .tp-price {
  display: inline-block;
  font-size: 24rpx;
}

.tp-title {
  width: 70%;
  color: rgb(81, 81, 81);
}

.tp-price {
  width: 30%;
  color: rgb(253, 82, 129);
  text-align: cntent;
  font-size: 24rpx;
}
