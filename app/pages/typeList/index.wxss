/* pages/typeList/index.wxss */

.tabContent {
  width: 100%;
  background: #fff;
  padding-top: 6rpx;
}

.tc-center-list {
  width: 50%;
  display: inline-block;
  margin-bottom: 20rpx;
}

.tc-cente-img {
  display: block;
  width: 320rpx;
  height: 320rpx;
  margin: 0 auto;
}

.tc-cente-text {
  width: 320rpx;
  margin: 0 auto;
}

.tc-title, .tc-price {
  display: inline-block;
  font-size: 24rpx;
}

.tc-title {
  width: 70%;
  color: rgb(81, 81, 81);
}

.tc-price {
  width: 30%;
  color: rgb(253, 82, 129);
  text-align: cntent;
  font-size: 24rpx;
}
.bottomLine{
  background-image: url(../img/bottomLine.jpg);
  background-size: 100%;
  height: 120rpx;
}
.bottomLine2{
  background-image: url(../img/bottomLine2.jpg);
  background-size: 100%;
  height: 120rpx;
}